{
    "version":2,
    "builds":[
        {
            "src":"portfolio/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runtime":"python3.10.5"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "disDir":"static_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":".static/&1"
        },
        {
            "src":"/(.*)",
            "dest":"portfolio/wsgi.py"
        }
    ]
}